    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dev": "webpack-dev-server",
    "test": "react-scripts test --watchAll=false --coverage --collectCoverageFrom='!src/**/*.{types,stories,constants,test,spec}.{ts,tsx}'",
    "devTest": "react-scripts test --watchAll --coverage --coverageReporters='json-summary' --collectCoverageFrom='!src/**/*.{types,stories,constants,test,spec}.{ts,tsx}'",
    "make-badges": "istanbul-badges-readme --coverageDir='./coverage' --readmeDir='./'",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "jest": {
    "coverageThreshold": {
      "global": {}
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.23.3",
    "@babel/plugin-transform-react-inline-elements": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.8",
    "@svgr/webpack": "^8.1.0",
    "@types/lodash": "^4.14.202",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "css-loader": "^6.9.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "istanbul-badges-readme": "^1.8.5",
    "mini-css-extract-plugin": "^2.7.7",
    "prettier": "^3.2.3",
    "style-loader": "^3.3.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.1.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
